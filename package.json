{
  "name": "azure-naming-conventions",
  "version": "0.1.3",
  "description": "library to test Azure resource names to meet the Azure naming conventions",
  "license": "MIT",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/andikrueger/AzureNamingConvention.git"
  },
  "homepage": "https://andikrueger.github.io/azure-naming-conventions",
  "bugs": {
    "url": "https://github.com/andikrueger/AzureNamingConvention/issues"
  },
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -w -p .",
    "test": "npm run build && nyc -r=lcov -r=text mocha \"dist/src/**/*.spec.js"
  },
  "keywords": [
    "Azure",
    "TypeScript"
  ],
  "maintainers": [
    {
      "name": "Andi Kruger",
      "email": "solutions@andikrueger.de",
      "url": "https://andikrueger.de"
    }
  ],
  "devDependencies": {
    "@types/chai": "^4.1.2",
    "@types/mocha": "^2.2.47",
    "@types/sinon": "^4.1.3",
    "@types/sinon-chai": "^2.7.29",
    "chai": "^4.1.2",
    "gulp": "^3.9.1",
    "mocha": "^4.1.0",
    "nyc": "^11.4.1",
    "sinon": "^4.2.2",
    "sinon-chai": "^2.14.0",
    "source-map-support": "^0.5.3",
    "ts-node": "^4.1.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js"
    ],
    "extensions": [
      ".js"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  }
}